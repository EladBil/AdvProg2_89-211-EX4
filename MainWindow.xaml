<Window x:Class="FlightSimADVProg2_ex1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FlightSimADVProg2_ex1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        xmlns:controls="clr-namespace:FlightSimADVProg2_ex1.SubViews">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>
        <controls:GraphsView Loaded="GraphsView_Loaded" x:Name="GraphsView"/>
        <controls:GroundRelativeView x:Name="GroundRelativeView" Grid.Column="1" Loaded="GroundRelativeView_Loaded"/>
    </Grid>
</Window>
